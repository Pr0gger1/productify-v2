{
	"name": "productify-university-project",
	"version": "2.2.1",
	"description": "todo app project based on TS, React and Firebase",
	"type": "module",
	"scripts": {
		"dev": "cross-env MODE=development vite serve",
		"build:prod": "cross-env MODE=production vite build",
		"build:dev": "cross-env MODE=development vite build",
		"preview": "vite preview",
		"predeploy": "npm run build:prod",
		"deploy": "firebase deploy",
		"prettier": "npx prettier . --write",
		"precommit": "npx lint-staged",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"dependencies": {
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.11.0",
		"@mui/icons-material": "^5.11.9",
		"@mui/material": "^5.14.2",
		"@mui/x-date-pickers": "^6.0.2",
		"@reduxjs/toolkit": "^1.9.3",
		"@tanstack/react-query": "^4.32.6",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^13.5.0",
		"assert": "^2.0.0",
		"components": "^0.1.0",
		"dayjs": "^1.11.7",
		"dotenv": "^16.0.3",
		"firebase": "^9.23.0",
		"firebase-tools": "^12.5.2",
		"http-proxy-middleware": "^2.0.6",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-favicon": "^1.0.1",
		"react-hook-form": "^7.45.4",
		"react-redux": "^8.0.5",
		"react-responsive": "^9.0.2",
		"react-router-dom": "^6.4.2",
		"react-transition-group": "^4.4.5",
		"styled-components": "^5.3.6",
		"util": "^0.12.5",
		"web-vitals": "^2.1.4"
	},
	"devDependencies": {
		"@babel/plugin-proposal-private-property-in-object": "^7.21.11",
		"@storybook/addon-essentials": "^7.3.2",
		"@storybook/addon-interactions": "^7.3.2",
		"@storybook/addon-links": "^7.3.2",
		"@storybook/addon-onboarding": "^1.0.8",
		"@storybook/blocks": "^7.3.2",
		"@storybook/preset-create-react-app": "^7.3.2",
		"@storybook/react": "^7.3.2",
		"@storybook/react-webpack5": "^7.3.2",
		"@storybook/testing-library": "^0.2.0",
		"@types/jest": "^29.2.5",
		"@types/node": "^18.11.18",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.10",
		"@types/styled-components": "^5.1.26",
		"@typescript-eslint/eslint-plugin": "^6.2.0",
		"@typescript-eslint/parser": "^6.2.0",
		"@vitejs/plugin-react": "^4.0.4",
		"autoprefixer": "^10.4.14",
		"babel-plugin-named-exports-order": "^0.0.2",
		"cross-env": "^7.0.3",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-react": "^7.33.0",
		"eslint-plugin-storybook": "^0.6.13",
		"husky": "^8.0.3",
		"i": "^0.3.7",
		"lint-staged": "^14.0.1",
		"optimize-plugin": "^1.3.1",
		"prettier": "^3.0.3",
		"prop-types": "^15.8.1",
		"sass": "^1.58.3",
		"sass-loader": "^13.2.2",
		"storybook": "^7.3.2",
		"ts-loader": "^9.4.4",
		"typescript": "^4.9.5",
		"typescript-plugin-css-modules": "^5.0.1",
		"vite": "^4.4.9",
		"vite-plugin-html": "^3.2.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx}": [
			"prettier --write",
			"git add"
		]
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"overrides": [
			{
				"files": [
					"**/*.stories.*"
				],
				"rules": {
					"import/no-anonymous-default-export": "off"
				}
			}
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
